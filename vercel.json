{
    "version": 2,
    "framework": null,
    "buildCommand": "composer install --no-dev --optimize-autoloader --no-interaction && php artisan config:clear && php artisan route:clear && php artisan view:clear",
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.7.2"
        }
    },
    "routes": [
        {
            "src": "/favicon\\.ico",
            "dest": "/public/favicon.ico"
        },
        {
            "src": "/robots\\.txt",
            "dest": "/public/robots.txt"
        },
        {
            "src": "/(.*)\\.(?:css|js|png|jpg|jpeg|gif|ico|svg|woff2?|ttf|eot|map)$",
            "dest": "/public/$1.$2"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ]
}
